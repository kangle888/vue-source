{"code":"import { createVNode } from \"./vnode\";\nexport function apiCreateApp(render) {\n    return function createApp(rootComponent, rootProps) {\n        const app = {\n            _component: rootComponent,\n            _props: rootProps,\n            _container: null,\n            mount(rootContainer) {\n                //组件的渲染\n                // 1、创建组件的虚拟dom\n                // 2、将虚拟dom转换成真实dom\n                // 3、将真实dom插入到rootContainer中\n                let vnode = createVNode(rootComponent, rootProps);\n                // console.log(vnode, \"打印vnode\");\n                render(vnode, rootContainer);\n                app._container = rootContainer;\n            },\n        };\n        return app;\n    };\n}\n","references":["E:/前端学习/vue-source/hand-write-vue3-source/packages/runtime-core/src/vnode.ts"],"dts":{"name":"E:/前端学习/vue-source/hand-write-vue3-source/node_modules/.cache/rollup-plugin-typescript2/placeholder/runtime-core/src/apiCreateApp.d.ts","writeByteOrderMark":false,"text":"export declare function apiCreateApp(render: any): (rootComponent: any, rootProps: any) => {\n    _component: any;\n    _props: any;\n    _container: any;\n    mount(rootContainer: any): void;\n};\n"}}
